{
  "//": "The production flag is added in the jenkins build script.",
  "name": "lms-sync-users",
  "version": "1.2.1",
  "description": "Node.js based API template application.",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test:integration": "cross-env NODE_ENV=development tape \"test/integration/**/*.js\" | tap-spec",
    "coverage": "cross-env NODE_ENV=development istanbul cover tape -- \"messages/*.js\" -- \"server/**/*.js\" -- \"test/**/*.js\"",
    "test:docker-unit": "apk update; apk add python make; npm install; npm test", 
    "test": "npx standard; tape \"test/unit/**/*.js\" | tap-spec",
    "start": "cross-env NODE_ENV=development nodemon app.js"
  },
  "resolutions": {
    "azure-arm-intune": "1.0.0-preview"
  },
  "dependencies": {
    "amqp10": "^3.6.0",
    "bluebird": "^3.5.1",
    "bunyan": "^1.8.12",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "inquirer": "^6.0.0",
    "kth-canvas-api": "^0.15.5",
    "kth-message-type": "^1.9.0",
    "kth-node-server": "^3.1.1",
    "moment": "^2.21.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "azure-common": "^0.9.20",
    "azure-sb": "^0.10.6",
    "blue-tape": "^1.0.0",
    "cross-env": "^5.1.4",
    "istanbul": "^0.4.5",
    "proxyquire": "^2.0.0",
    "randomstring": "^1.1.5",
    "rewire": "^4.0.1",
    "rewire-global": "^1.0.11",
    "sinon": "^5.0.10",
    "standard": "^11.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "4.3.1"
  }
}

{
  "//": "The production flag is added in the jenkins build script.",
  "name": "lms-sync-users",
  "version": "1.2.1",
  "description": "Node.js based API template application.",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test:integration": "cross-env NODE_ENV=development tape \"test/integration/**/*.js\" | tap-spec",
    "coverage": "cross-env NODE_ENV=development istanbul cover tape -- \"messages/*.js\" -- \"server/**/*.js\" -- \"test/**/*.js\"",
    "test": "standard; tape \"test/unit/**/*.js\" | tap-spec",
    "test:docker-unit": "apk update; apk add python make; npm install --no-optional; npm test",
    "test:docker-integration": "apk update; apk add python make; npm install --no-optional; npm run test:integration",
    "start": "cross-env NODE_ENV=development nodemon app.js",
    "debug": "NODE_ENV=development node --nolazy --inspect-brk=9229 app.js | bunyan -o short"
  },
  "resolutions": {
    "azure-arm-intune": "1.0.0-preview"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "bunyan": "^1.8.12",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "inquirer": "^6.2.0",
    "kth-canvas-api": "^0.15.6",
    "kth-message-type": "^1.9.0",
    "kth-node-server": "^3.1.1",
    "moment": "^2.21.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rhea": "^0.3.4",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "azure-common": "^0.9.20",
    "azure-sb": "^0.10.6",
    "blue-tape": "^1.0.0",
    "cross-env": "^5.2.0",
    "istanbul": "^0.4.5",
    "nodemon": "^1.18.7",
    "proxyquire": "^2.1.0",
    "randomstring": "^1.1.5",
    "rewire": "^2.5.2",
    "rewire-global": "^1.0.11",
    "sinon": "^5.1.1",
    "standard": "^11.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "engines": {
    "node": "4.3.1"
  }
}
